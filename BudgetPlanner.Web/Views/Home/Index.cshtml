@model HomeViewModel

@{
    ViewBag.Title = "Dashboard";
    var budgetPlannerData = Html.Base64Encode(Json.Serialize(new BudgetPanelDashboardListViewModel { 
        AccountId = Model.AccountId,
        LastUpdated = Model.LastUpdated }), Encoding.UTF8);
}

<section data-src="@Url.Action("BudgetPlannerListDashboard","ViewComponent")"
         data-parameters="@budgetPlannerData">
    <div class="spinner-border" role="status">
        <span class="sr-only">Loading...</span>
    </div>
</section>